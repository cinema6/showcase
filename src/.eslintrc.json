{
    "extends": ["airbnb"],
    "rules": {
        "react/jsx-indent": [
            "error",
            4
        ],
        "react/jsx-indent-props": [
            "error",
            4
        ],
        "indent": [
            "error",
            4
        ],
        "import/no-unresolved": [
            2,
            {
                "commonjs": true,
                "ignore": ["adwords-track-conversion"]
            }
        ]
    }
}
